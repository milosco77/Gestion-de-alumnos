@model IEnumerable<MVCCore.DatabaseFirstModel.ListadoAsignaturas>

@{
    ViewData["Title"] = "Listado de Asignaturas";
}

<h1>Listado de Asignaturas</h1>

<p>
    <a asp-action="Create">Crear Nuevo</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ListadoAsignaturasId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Codigo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Nombre)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Creditos)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Horas)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Correlativas)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Categoria)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ListadoCarreras)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ListadoAsignaturasId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Codigo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Nombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Creditos)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Horas)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Correlativas)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Categoria)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ListadoCarreras.Nombre)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ListadoAsignaturasId">Editar</a> |
                <a asp-action="Details" asp-route-id="@item.ListadoAsignaturasId">Detalles</a> |
                <a asp-action="Delete" asp-route-id="@item.ListadoAsignaturasId">Borrar</a>
            </td>
        </tr>
}
    </tbody>
</table>
